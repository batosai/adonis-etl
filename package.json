{
  "name": "@jrmc/adonis-etl",
  "version": "1.0.1",
  "keywords": [
    "adonisjs",
    "etl",
    "ace",
    "commands"
  ],
  "author": "Jeremy Chaufourier jeremy@chaufourier.fr",
  "license": "MIT",
  "description": "AdonisJS ETL skaffold commands package - Generate ETL (Extract, Transform, Load) components for your AdonisJS applications.",
  "type": "module",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batosai/adonis-etl.git"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "npm run clean && tsc && npm run copy-stubs",
    "copy-stubs": "cp -r stubs/make build/stubs/",
    "postbuild": "adonis-kit index build/commands",
    "prepublishOnly": "npm run build",
    "clean": "del-cli build",
    "format": "prettier --write ."
  },
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./commands/*": "./build/commands/*.js"
  },
  "dependencies": {
    "@jrmc/etl": "^1.0.1"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.19.0",
    "@adonisjs/lucid": "^21.7.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^1.4.1",
    "@types/node": "^24.0.11",
    "del-cli": "^7.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "volta": {
    "node": "22.17.0"
  }
}
